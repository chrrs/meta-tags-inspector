{
	"name": "meta-tags-inspector",
	"version": "1.0.0",
	"repository": "github:chrrs/meta-tags-inspector",
	"license": "BSD-3-Clause",
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build",
		"release": "yarn release:v2 && yarn release:v3",
		"release:v2": "cross-env NODE_ENV=production MANIFEST=v2 vite build && cd dist/ext && bestzip ../extension-v2.zip *",
		"release:v3": "cross-env NODE_ENV=production MANIFEST=v3 vite build && cd dist/ext && bestzip ../extension-v3.zip *",
		"format": "prettier --ignore-path .gitignore --write ."
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tippyjs/react": "^4.2.6",
		"@types/cheerio": "^0.22.31",
		"cheerio": "^1.0.0-rc.11",
		"react": "^17.0.2",
		"react-dom": "^18.1.0",
		"react-icons": "^4.4.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-react-jsx": "^7.17.12",
		"@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
		"@types/chrome": "^0.0.180",
		"@types/node": "^17.0.36",
		"@types/react": "^17.0.33",
		"@types/react-dom": "^18.0.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vitejs/plugin-react": "^1.3.2",
		"babel-plugin-macros": "^3.1.0",
		"bestzip": "^2.2.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.4.0",
		"prettier": "^2.5.1",
		"tailwindcss": "^3.0.24",
		"twin.macro": "^2.8.2",
		"typescript": "^4.5.5",
		"vite": "^2.9.9",
		"vite-plugin-static-copy": "^0.5.0",
		"vite-tsconfig-paths": "^3.5.0"
	}
}
